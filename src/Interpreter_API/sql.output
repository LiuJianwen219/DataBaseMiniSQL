Grammar

    0 $accept: stmt_list $end

    1 stmt_list: stmt ';'
    2          | stmt_list stmt ';'

    3 stmt: insert_stmt

    4 insert_stmt: INSERT INTO NAME VALUES insert_vals_list

    5 insert_vals_list: '(' insert_vals ')'
    6                 | insert_vals_list ',' '(' insert_vals ')'

    7 insert_vals: expr
    8            | insert_vals ',' expr

    9 stmt: drop_table_stmt

   10 drop_table_stmt: DROP TABLE NAME

   11 stmt: drop_index_stmt

   12 drop_index_stmt: DROP INDEX NAME

   13 stmt: quit_stmt

   14 quit_stmt: QUIT

   15 stmt: execfile_stmt

   16 execfile_stmt: EXECFILE STRING

   17 stmt: create_index_stmt

   18 create_index_stmt: CREATE INDEX NAME ON expr_index_attr

   19 expr: NAME
   20     | NAME '.' NAME
   21     | STRING
   22     | INTNUM
   23     | APPROXNUM
   24     | BOOL

   25 expr_index_attr: NAME '(' NAME ')'


